{
  "npm-scripts.showStartNotification": false,
  "git.ignoreLimitWarning": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "build": true,
    ".vscode": true,
    "node_modules": true,
    "scripts": true,
    ".github": true,
    ".github/workflows": true,
    ".husky": true
  },
  "hide-files.files": [
    "build",
    ".vscode",
    "node_modules",
    "scripts",
    ".github",
    ".github/workflows",
    ".husky"
  ],
  "i18n-ally.localesPaths": ["public/locales", "src/shared/config/i18n"],
  "i18n-ally.sourceLanguage": "ru"
}
